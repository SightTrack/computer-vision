  "label_smoothing": 0.1,
  "device": "cpu",
  "num_workers": 2,
  "use_mixup": true,
  "mixup_alpha": 0.4,
  "use_cutmix": true,
  "cutmix_alpha": 1.0,
  "target_level": "family"
}